<div class="alert alert-info">
	<strong>Recherche : </strong>{{ search }}
</div>

<table class="table table-striped table-hover">
	<tbody>
		{% for user in users %}
			<tr>
				<td><img src="{{ user.avatar_url }}" style="width:32px; height:32px"></td>
				<td>{{ user.login }}</td>
				<td class="text-right">
					<a href="{{ path('user', {name:user.login}) }}" class="btn btn-info">
						<strong>Voir</strong>
					</a>
				</td>
			</tr>
		{% else %}
			<tr>
				<td colspan="3" class="text-danger">Aucun r√©sultat</td>
			</tr>
		{% endfor %}
	</tbody>
</table>